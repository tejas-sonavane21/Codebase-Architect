# Copy this file to .env and add your Gemini API key
GEMINI_API_KEY=your_api_key_here

# Gemini model to use (optional, has default)
GEMINI_MODEL=gemma-3-27b-it

# Rate limiting mode: safe (default) or fast
# safe = optimized for free tier (longer delays)
# fast = 70% reduced delays (for paid tier or testing)
DELAY_MODE=safe

# TPM (Tokens Per Minute) limit for the model
# gemma-3-*: 15000 (default)
# gemini-2.5-flash-lite: 250000
TPM_LIMIT=15000