# Codebase Architect - Environment Configuration
# Copy this file to .env and fill in your values

# === Gemini Web API Authentication ===
# Extract cookies from browser DevTools > Application > Cookies > https://gemini.google.com
GEMINI_SECURE_1PSID=your_cookie_value_here
GEMINI_SECURE_1PSIDTS=your_cookie_value_here

# === Model Configuration ===
GEMINI_WEB_MODEL=gemini-3.0-pro

# === Debug Options ===
DEBUG_FAILED_DIAGRAMS=true

# === Developer Mode ===
# When true, enables gem management commands (--gems-list, --gems-sync, etc.)
DEV_MODE=true

# === Console Verbocity ===
# Console verbosity: 0=silent, 1=normal (default), 2=verbose
CONSOLE_VERBOSITY=1